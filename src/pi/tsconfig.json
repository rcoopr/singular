{
  "extends": "@tsconfig/node20/tsconfig.json",
  "compilerOptions": {
    "customConditions": ["browser"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "es2022"],
    "module": "es2022",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "target": "es2022"
  },
  "include": ["./"]
}
